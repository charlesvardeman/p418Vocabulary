<?xml version="1.0" encoding="utf-8" ?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:dc="http://purl.org/dc/elements/1.1/"
         xmlns:dcterms="http://purl.org/dc/terms/"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:schema="http://schema.org/"
         xmlns:datacite="http://purl.org/spar/datacite"
         xmlns:r3d="http://www.re3data.org/schema/3-0#"
         xmlns:gdx="https://geodex.org/voc/">

  <owl:Ontology rdf:about="gdx:">
    <dc:description>Schema.org extension for a Information Service Provider</dc:description>
    <dc:title>Information Service Provider Vocabulary</dc:title>
    <dc:creator>EarthCube Registry Working Group</dc:creator>
    <dcterms:issued rdf:datatype="xsd:date">2017-10-16</dcterms:issued>
    <owl:versionInfo>0.1.0</owl:versionInfo>
  </owl:Ontology>

  <owl:Class rdf:about="gdx:InformationService">
    <rdfs:subClassOf rdf:resource="schema:Service"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:label xml:lang="en">Information Service</rdfs:label>
    <rdfs:comment xml:lang="en">A provider of information as a service to a community.</rdfs:comment>
  </owl:Class>

  <owl:ObjectProperty rdf:about="gdx:informationProvider">
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:subPropertyOf rdf:resource="schema:provider"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:domain rdf:resource="gdx:InformationService"/>
    <rdfs:range rdf:resource="gdx:InformationResourceRepository"/>
    <rdfs:label xml:lang="en">Information Resource Respository</rdfs:label>
    <rdfs:comment xml:lang="en">An organization acting as a repository that provides access to certain types of information resources for specific subject areas.</rdfs:comment>
  </owl:ObjectProperty>

  <owl:Class rdf:about="gdx:InformationResourceRepository">
    <rdfs:subClassOf rdf:resource="schema:Organization"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="gdx:repositoryName"/>
        <owl:minCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:minCardinality>
        <owl:maxCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="gdx:repositoryUrl"/>
        <owl:minCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:minCardinality>
        <owl:maxCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="gdx:description"/>
        <owl:maxCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="gdx:repositoryType"/>
        <owl:minCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="gdx:size"/>
        <owl:maxCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="gdx:startDate"/>
        <owl:maxCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="gdx:endDate"/>
        <owl:maxCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="gdx:closedDate"/>
        <owl:maxCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="gdx:offlineDate"/>
        <owl:maxCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="gdx:userInterfaceLanguage"/>
        <owl:minCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="gdx:subjectArea"/>
        <owl:minCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="gdx:missionStatementUrl"/>
        <owl:maxCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="gdx:providerType"/>
        <owl:minCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:minCardinality>
        <owl:maxCardinality rdf:datatype="xsd:nonNegativeInteger">2</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:label xml:lang="en">Data Repository</rdfs:label>
    <rdfs:comment xml:lang="en">A Data Repository.</rdfs:comment>
    <skos:related rdf:resource="r3d:repository"/>
  </owl:Class>

  <owl:ObjectProperty rdf:about="gdx:identifier">
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:subPropertyOf rdf:resource="schema:identifier"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:domain rdf:resource="gdx:InformationResourceRepository"/>
    <rdfs:range rdf:resource="gdx:Identifier"/>
    <rdfs:label xml:lang="en">Identifier</rdfs:label>
    <rdfs:comment xml:lang="en">Links to identifiers that uniquely identify the data repository.</rdfs:comment>
    <skos:related rdf:resource="r3d:identifiers"/>
  </owl:ObjectProperty>

  <owl:Class rdf:about="gdx:Identifier">
    <rdfs:subClassOf rdf:resource="owl:Thing"/>
    <skos:exactMatch rdf:resource="datacite:Identifier"/>
    <skos:closeMatch rdf:resource="schema:PropertyValue"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="identifierType"/>
        <owl:minCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:minCardinality>
        <owl:maxCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="gdx:identifierValue"/>
        <owl:minCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:minCardinality>
        <owl:maxCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:label xml:lang="en">Data Repository Identifier</rdfs:label>
    <rdfs:comment xml:lang="en">A unique identifier with a type and value for the Data Repository.</rdfs:comment>
    <skos:related rdf:resource="r3d:identifiers"/>
    <skos:related rdf:resource="r3d:repositoryIdentifier"/>
  </owl:Class>

  <owl:ObjectProperty rdf:about="gdx:identifierType">
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:subPropertyOf rdf:resource="schema:propertyID"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:domain rdf:resource="gdx:Identifier"/>
    <rdfs:range rdf:resource="gdx:IdentifierType"/>
    <rdfs:label xml:lang="en">An identifier's type such as DOI, ARK, URN or for custom types like Re3Data. For example a DOI of doi:10.1234/567890 would have the identifier value of '&lt;http://example.org/voc/IdentifierTypeDOI&gt;'</rdfs:label>
    <rdfs:comment xml:lang="en">The type that identifies the domain space for its uniqueness.</rdfs:comment>
    <skos:related rdf:resource="r3d:repositoryIdentifierType"/>
  </owl:ObjectProperty>

  <owl:Class rdf:about="gdx:IdentifierType">
    <rdfs:subClassOf rdf:resource="owl:Thing"/>
    <skos:exactMatch rdf:resource="datacite:IdentifierScheme"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:label xml:lang="en">Data Repository Identifier Type</rdfs:label>
    <rdfs:comment xml:lang="en">A identifier type for an Identifier.</rdfs:comment>
    <skos:related rdf:resource="r3d:repositoryIdentifierType"/>
  </owl:Class>

  <owl:namedIndividual rdf:about="gdx:IdentifierTypeDOI">
    <rdf:type rdf:resource="gdx:IdentifierType"/>
    <skos:exactMatch rdf:resource="datacite:doi"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:label xml:lang="en">Digital Object Identifier (DOI)</rdfs:label>
    <rdfs:comment xml:lang="en">An identifier minted by an issuer of Digital Object Identifiers (DOIs).</rdfs:comment>
  </owl:namedIndividual>

  <owl:namedIndividual rdf:about="gdx:IdentifierTypeRe3data">
    <rdf:type rdf:resource="gdx:IdentifierType"/>
    <skos:broadMatch rdf:resource="gdx:IdentifierTypeDOI"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:label xml:lang="en">Re3data Digital Object Identifier (DOI)</rdfs:label>
    <rdfs:comment xml:lang="en">A DOI issued by Re3Data for a Data Repository's metadata record at Re3data.</rdfs:comment>
    <skos:related rdf:resource="r3d:doi"/>
  </owl:namedIndividual>

  <owl:DatatypeProperty rdf:about="gdx:identifierValue">
    <rdf:type rdf:resource="rdf:Property"/>
    <skos:exactMatch rdf:resource="schema:value"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:domain rdf:resource="gdx:Identifier"/>
    <rdfs:range rdf:resource="xsd:string"/>
    <rdfs:label xml:lang="en">An identifier's value.</rdfs:label>
    <rdfs:comment xml:lang="en">The value that will be unique within a specifc identifier type. For example a DOI of doi:10.1234/567890 would have the identifier value of '10.1234/567890'</rdfs:comment>
    <skos:related rdf:resource="r3d:repositoryIdentifierValue"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="gdx:identifierUrl">
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:subPropertyOf rdf:resource="schema:url"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:domain rdf:resource="gdx:Identifier"/>
    <rdfs:range rdf:resource="xsd:anyURI"/>
    <rdfs:label xml:lang="en">An identifier's resolvable URL.</rdfs:label>
    <rdfs:comment xml:lang="en">The url that will resolve to the identified thing. For example a DOI of doi:10.1234/567890 could have the identifier url of 'https://doi.org/10.1234/567890' and/or 'http://doi.org/10.1234/567890' and/or 'http://dx.doi.org/10.1234/567890', hence it's unbounded cardinality.</rdfs:comment>
    <skos:related rdf:resource="r3d:repositoryIdentifierValue"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="gdx:repositoryName">
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:subPropertyOf rdf:resource="schema:name"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:domain rdf:resource="gdx:InformationResourceRepository"/>
    <rdfs:range rdf:resource="xsd:string"/>
    <rdfs:label xml:lang="en">Repository Name</rdfs:label>
    <rdfs:comment xml:lang="en">The name of the data repository expressed in text.</rdfs:comment>
    <skos:related rdf:resource="r3d:repositoryName"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="gdx:additionalName">
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:subPropertyOf rdf:resource="schema:additionalName"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:domain rdf:resource="gdx:InformationResourceRepository"/>
    <rdfs:range rdf:resource="xsd:string"/>
    <rdfs:label xml:lang="en">Additional Name</rdfs:label>
    <rdfs:comment xml:lang="en">The alternative name or acronym for the data repository.</rdfs:comment>
    <skos:related rdf:resource="r3d:alternateName"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="gdx:repositoryUrl">
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:subPropertyOf rdf:resource="schema:url"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:domain rdf:resource="gdx:InformationResourceRepository"/>
    <rdfs:range rdf:resource="xsd:anyURI"/>
    <rdfs:label xml:lang="en">Data Repository URL</rdfs:label>
    <rdfs:comment xml:lang="en">The canonical URL of the data repository.</rdfs:comment>
    <skos:related rdf:resource="r3d:repositoryUrl"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="gdx:description">
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:subPropertyOf rdf:resource="schema:description"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:domain rdf:resource="gdx:InformationResourceRepository"/>
    <rdfs:range rdf:resource="rdf:langString"/>
    <rdfs:label xml:lang="en">Data Repository Description</rdfs:label>
    <rdfs:comment xml:lang="en">A textual description providing additional information about the Data Repository (primary language is English). The language should be defined by the xsd datatype of the value. For example, '@en'. See </rdfs:comment>
    <skos:related rdf:resource="r3d:description"/>
  </owl:DatatypeProperty>

  <owl:ObjectProperty rdf:about="gdx:contactPoint">
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:subPropertyOf rdf:resource="schema:contactPoint"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:domain rdf:resource="gdx:InformationResourceRepository"/>
    <rdfs:range rdf:resource="gdx:ContactPoint"/>
    <rdfs:label xml:lang="en">Data Repository Contact</rdfs:label>
    <rdfs:comment xml:lang="en">The agent to contact.</rdfs:comment>
    <skos:related rdf:resource="r3d:repositoryContact"/>
  </owl:ObjectProperty>

  <owl:Class rdf:about="gdx:ContactPoint">
    <rdfs:subClassOf rdf:resource="schema:ContactPoint"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:label xml:lang="en">Contact Point</rdfs:label>
    <rdfs:comment xml:lang="en">The contact point with either an email address or URL for an online contact form.</rdfs:comment>
    <skos:related rdf:resource="r3d:repositoryContact"/>
  </owl:Class>

  <owl:DatatypeProperty rdf:about="gdx:contactEmail">
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:subPropertyOf rdf:resource="schema:email"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:domain rdf:resource="gdx:ContactPoint"/>
    <rdfs:range rdf:resource="xsd:string"/>
    <rdfs:label xml:lang="en">Contact Point Email</rdfs:label>
    <rdfs:comment xml:lang="en">An email address for a data repository's contact point.</rdfs:comment>
    <skos:related rdf:resource="r3d:repositoryContact"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="gdx:contactUrl">
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:subPropertyOf rdf:resource="schema:url"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:domain rdf:resource="gdx:ContactPoint"/>
    <rdfs:range rdf:resource="xsd:anyURI"/>
    <rdfs:label xml:lang="en">Contact Point URL</rdfs:label>
    <rdfs:comment xml:lang="en">URL for an online contact form.</rdfs:comment>
    <skos:related rdf:resource="r3d:repositoryContact"/>
  </owl:DatatypeProperty>

  <owl:ObjectProperty rdf:about="gdx:repositoryType">
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:subPropertyOf rdf:resource="schema:additionalType"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:domain rdf:resource="gdx:InformationResourceRepository"/>
    <rdfs:range rdf:resource="gdx:RepositoryType"/>
    <rdfs:label xml:lang="en">Data Repository Type</rdfs:label>
    <rdfs:comment xml:lang="en">The type of repository.</rdfs:comment>
    <skos:related rdf:resource="r3d:type"/>
  </owl:ObjectProperty>

  <owl:Class rdf:about="gdx:RepositoryType">
    <rdf:subClassOf rdf:resource="owl:Thing"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:label xml:lang="en">Repostory Type</rdfs:label>
    <rdfs:comment xml:lang="en">The type of repository.</rdfs:comment>
    <skos:related rdf:resource="r3d:type"/>
  </owl:Class>

  <owl:namedIndividual rdf:about="gdx:RepositoryTypeDisciplinary">
    <rdf:type rdf:resource="gdx:RepositoryType"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:label xml:lang="en">Disciplinary repository type</rdfs:label>
  </owl:namedIndividual>

  <owl:namedIndividual rdf:about="gdx:RepositoryTypeGovernmental">
    <rdf:type rdf:resource="gdx:RepositoryType"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:label xml:lang="en">Governmental repository type</rdfs:label>
  </owl:namedIndividual>

  <owl:namedIndividual rdf:about="gdx:RepositoryTypeInstitutional">
    <rdf:type rdf:resource="gdx:RepositoryType"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:label xml:lang="en">Institutional repository type</rdfs:label>
  </owl:namedIndividual>

  <owl:namedIndividual rdf:about="gdx:RepositoryTypeMultidisciplinary">
    <rdf:type rdf:resource="gdx:RepositoryType"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:label xml:lang="en">Multidisciplinary repository type</rdfs:label>
  </owl:namedIndividual>

  <owl:namedIndividual rdf:about="gdx:RepositoryTypeProjectRelated">
    <rdf:type rdf:resource="gdx:RepositoryType"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:label xml:lang="en">Project-related repository type</rdfs:label>
  </owl:namedIndividual>

  <owl:namedIndividual rdf:about="gdx:RepositoryTypeOther">
    <rdf:type rdf:resource="gdx:RepositoryType"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:label xml:lang="en">Other repository type</rdfs:label>
  </owl:namedIndividual>

  <owl:DatatypeProperty rdf:about="gdx:totalNumberOfItemsProvided">
    <rdf:type rdf:resource="rdf:Property"/>
    <skos:closeMatch rdf:resource="schema:numberOfItems"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:domain rdf:resource="gdx:InformationResourceRepository"/>
    <rdfs:range rdf:resource="xsd:integer"/>
    <rdfs:label xml:lang="en">Size of Data Repository</rdfs:label>
    <rdfs:comment xml:lang="en">The total number of items provided by the repository.</rdfs:comment>
    <skos:related rdf:resource="r3d:size"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="gdx:startDate">
    <rdf:type rdf:resource="rdf:Property"/>
    <skos:closeMatch rdf:resource="schema:foundingDate"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:domain rdf:resource="gdx:InformationResourceRepository"/>
    <rdfs:range rdf:resource="xsd:date"/>
    <rdfs:label xml:lang="en">Start Date</rdfs:label>
    <rdfs:comment xml:lang="en">Releasing date of the repository.</rdfs:comment>
    <skos:related rdf:resource="r3d:startDate"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="gdx:endDate">
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:domain rdf:resource="gdx:InformationResourceRepository"/>
    <rdfs:range rdf:resource="xsd:date"/>
    <rdfs:label xml:lang="en">End Date</rdfs:label>
    <rdfs:comment xml:lang="en">The date the repository ended its service of ingesting new research data and/or providing it.</rdfs:comment>
    <skos:related rdf:resource="r3d:endDate"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="gdx:closedDate">
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:domain rdf:resource="gdx:InformationResourceRepository"/>
    <rdfs:range rdf:resource="xsd:date"/>
    <rdfs:label xml:lang="en">Closed Date</rdfs:label>
    <rdfs:comment xml:lang="en">The date when the repository stopped ingesting new research data to its database. The database and its research data are still available.</rdfs:comment>
    <skos:related rdf:resource="r3d:closed"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="gdx:offlineDate">
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:closeMatch rdf:resource="schema:dissolutionDate"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:domain rdf:resource="gdx:InformationResourceRepository"/>
    <rdfs:range rdf:resource="xsd:date"/>
    <owl:maxCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:maxCardinality>
    <rdfs:label xml:lang="en">Offline Date</rdfs:label>
    <rdfs:comment xml:lang="en">The date when the repository went offline. The database and/or the website are no longer available.</rdfs:comment>
    <skos:related rdf:resource="r3d:offline"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="gdx:userInterfaceLanguage">
    <rdf:type rdf:resource="rdf:Property"/>
    <rdfs:subPropertyOf rdf:resource="schema:inLanguage"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:domain rdf:resource="gdx:InformationResourceRepository"/>
    <rdfs:range rdf:resource="xsd:language"/>
    <rdfs:label xml:lang="en">Repository Language</rdfs:label>
    <rdfs:comment xml:lang="en">The user interface language(s) of the data repository.</rdfs:comment>
    <skos:related rdf:resource="r3d:repositoryLanguage"/>
  </owl:DatatypeProperty>

  <owl:ObjectProperty rdf:about="gdx:subjectArea">
    <rdf:type rdf:resource="rdf:Property"/>
    <skos:closeMatch rdf:resource="schema:keywords"/>
    <rdfs:isDefinedBy rdf:resource="gdx:"/>
    <rdfs:domain rdf:resource="gdx:InformationResourceRepository"/>
    <rdfs:range rdf:resource="gdx:SubjectArea"/>
    <rdfs:label xml:lang="en">Subject Area</rdfs:label>
    <rdfs:comment xml:lang="en">The disciplinary focus of the repository.</rdfs:comment>
    <skos:related rdf:resource="r3d:subject"/>
  </owl:ObjectProperty>

  <owl:Class rdf:about="gdx:SubjectArea">
    <rdfs:subClassOf rdf:resource="owl:Thing"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="gdx:subjectScheme"/>
        <owl:maxCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="gdx:subjectId"/>
        <owl:maxCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="gdx:subjectName"/>
        <owl:minCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:minCardinality>
        <owl:maxCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:maxCardinality>
